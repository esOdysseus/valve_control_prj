/****************************
 * Alias-naming Description.
 */
{
    "aliases": {
        "alias_udp_valve": {
            "provider-type": "udp",     // available-type : udp, tcp, uds, vsomeip, iotivity
            "address": {                // describe address of alias.
                "ip": "192.168.1.2",    // ip of alias that is destination.
                "mask": "24",           // Network mask.
                "port": "23456"         // port of alias.
            }
        },
        "alias_udp_01": {               // name of alias.
            "provider-type": "udp",     // available-type : udp, tcp, uds, vsomeip, iotivity
            "address": {                // describe address of alias.
                "ip": "192.168.1.2",    // ip of alias that is destination.
                "mask": "24",           // Network mask.
                "port": "12346"         // port of alias.
            }
        },
        "alias_tcp_01": {               // name of alias.
            "provider-type": "tcp",     // available-type : udp, tcp, uds, vsomeip, iotivity
            "address": {                // describe address of alias.
                "ip": "192.168.1.2", // ip of alias that is destination.
                "mask": "24",           // Network mask.
                "port": "12346"         // port of alias.
            }
        },
        "alias_vsomeip_01": {
            "provider-type": "vsomeip", // available-type : udp, tcp, uds, vsomeip, iotivity
            "address": {
                "service-id": "0x1234",
                "instance-id": "0x5678"
            } // atention : event-id, event-grp is described by protocol for service-oriented COMM.
        }
    }
}